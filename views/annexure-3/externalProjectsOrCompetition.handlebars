            <input id="contribution" type="text" name="contribution" value="{{contribution}}"  class="validate"
                required>
            <label for="contribution">Contribution</label>
        </div>
    </div>


    <div class="row">

        <div class="input-field col s6 md5">
            <input id="university" type="text" name="university" value="{{university}}"  class="validate"
                required>
            <label for="university">University</label>
        </div>

        <div class="input-field col s6 md5">
            <input id="duration" type="number" name="duration" value="{{duration}}"  class="validate" required>
            <label for="duration">Duration</label>
        </div>
    </div>

    <div class="row">

        <div class="input-field col s12">
            <input id="comments" type="text" name="comments" value="{{comments}}"  class="validate" required>
            <label for="comments">Comments</label>
        </div>
    </div>
    <div>
        <p>
            <label>
                <input id="indeterminate-checkbox" type="checkbox" />
                <span>I acknowledge that above filled information is verified and justified by me.</span>
            </label>
        </p>
    </div>
    <a href="/annexure-3/externalProjectsOrCompetition" class="waves-effect waves-light btn center red">Not
        Applicable</a>
    <button class="btn waves-effect waves-light" type="submit" id="submit_btn" disabled>Submit
    </button>
</div>
</div>
</div>
