<h5>Annexure 1</h5><br>
<div class="container">
    <h6>1.10 Industrial Visit Activities</h6>
    <hr>
    {{#if result}}
    {{#each result}}
    <div class="row" id="industrialVisitActivities">
        <div class="row">
            <div class="input-field col s6 md5">
                <input id="industrial-visit-role" type="text" name="industrial_visit_role"
                    value="{{industrial_visit_role}}" class="validate" required>
                <label for="industrial-visit-role">Role</label>
            </div>

            <div class="input-field col s6">
                <select name="class_name">
                    <option value="{{class_name}}" disabled selected>{{class_name}}</option>
                    <option value="FE">FE</option>
                    <option value="SE">SE</option>
                    <option value="TE">TE</option>
                    <option value="BE">BE</option>

                </select>
                <label for="">Class</label>

            </div>
        </div>

        <div class="row">

            <div class="input-field col s6">
                <select name="department">
                    <option value="{{department}}" disabled selected>{{department}}</option>
                    <option value="Information Technology">Information Technology</option>
                    <option value="Computer Engineering">Computer Engineering</option>
                    <option value="Mechanical Engineering">Mechanical Engineering</option>
                    <option value="Civil Engineering">Civil Engineering</option>
                    <option value="EXTC Engineering">EXTC Engineering</option>
                </select>
                <label for="">Department</label>
            </div>

            <div class="input-field col s6 md5">
                <input id="industrial-visit-days" type="number" name="industrial_visit_days"
                    value="{{industrial_visit_days}}" class="validate" required>
                <label for="industrial-visit-days">No. of Days</label>
            </div>

        </div>
        <div class="row">

            <div class="input-field col s6 md5">
                <select name="industrial_visit_organizer">
                    <option value="{{industrial_visit_organizer}}" disabled selected>{{industrial_visit_organizer}}
                    </option>
                    <option value="IEEE">IEEE</option>
                    <option value="ITSA">ITSA</option>
                    <option value="TPO">TPO</option>
                    <option value="Others">Others</option>
                </select>
                <label for="">Organized By</label>
            </div>

            <div class="input-field col s6 md5">
                <input id="name-of-company" type="text" name="name_of_company" value="{{name_of_company}}"
                    class="validate" required>
                <label for="name-of-company">Company Visited &nbsp; <a href="#" class="tooltipped"
                        data-tooltip="Name of company visited"><i class="fa fa-info-circle"></i></a></label>
            </div>

        </div>
        <div class="row">
            <div class="input-field col s6">
                <textarea id="iv-description" name="iv_description" aria-valuetext="{{iv_description}}"
                    class="materialize-textarea">{{iv_description}}</textarea>
                <label for="iv-description">Description</label>
            </div>
            <div class="input-field col s6 md5">
                <input id="industrial-visit-hrs" type="number" name="industrial_visit_hrs"
                    value="{{industrial_visit_hrs}}" class="validate" required>
                <label for="industrial-visit-hrs">Duration(hours)</label>
            </div>


        </div>
        <div class="row">

            <div class="input-field col s6 md5">
                <input id="iv-start-date" type="text" name="iv_start_date" value="{{iv_start_date}}"
                    class="datepicker validate" required>
                <label for="student-event-start-date">Start Date</label>
            </div>

            <div class="input-field col s6 md5">
                <input id="iv-end-date" type="text" name="iv_end_date" value="{{iv_end_date}}"
                    class="datepicker validate" required>
                <label for="student-event-end-date">End Date</label>
            </div>
        </div>
        <a href="/annexure-1/industrialVisitActivities/edit/{{id}}"
            class="btn waves-effect waves-light red center mb-2">Edit</a>
        <a href="/users/faculty/facultyOverview" class="btn waves-effect waves-light purple center mb-2">Back to
            overview</a><br><br>
        <form method="post" action="/annexure-1/industrialVisitActivities/delete/{{id}}?_method=DELETE">
            <input type="hidden" name="_method" value="DELETE">
            <input type="submit" class="btn waves-effect waves-light center red white-text" value="Delete">
        </form>
    </div>
</div>
{{else}}
{{/each}}
{{else}}
