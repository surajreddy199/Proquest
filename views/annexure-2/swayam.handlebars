            </div>

            <div class="input-field col s6">
                <input id="swayam_date" type="text" name="swayam_date" value="{{swayam_date}}"  class="datepicker validate"
                    required>
                <label for="swayam_date">Month / Year</label>
            </div>
        </div>

        <div class="row">
            <div class="input-field col s6 md5">
                <input id="swayam_duartion" type="number" name="swayam_duartion" value="{{swayam_duartion}}" 
                    class="validate " required>
                <label for="swayam_duartion" class="">Duration Of Course</label>
            </div>

            <div class="input-field col s6">
                <select name="certification_status" required>
                    <option value="{{certification_status}}" selected>{{certification_status}}</option>
                    <option value="YES" >YES</option>
                    <option value="NO" >NO</option>
